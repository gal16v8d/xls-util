<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
     "-//Checkstyle//DTD SuppressionFilter Configuration 1.0//EN"
     "https://checkstyle.org/dtds/suppressions_1_0.dtd">

<suppressions>
	<suppress checks="JavadocPackage" files=".*\.java" />
	<suppress checks="JavadocVariable" files=".*\.java" />
	<suppress checks="MissingJavadocMethod" files=".*\.java" />
	<suppress checks="DesignForExtension" files=".*\.java" />
	<suppress checks="FinalParameters" files=".*\.java" />
	<!-- Suppress conflict between spotless formatting and check -->
	<suppress checks="LineLength" files=".*\.java" />
	<!-- False positive using lombok -->
	<suppress checks="HideUtilityClassConstructor" files=".*\.java" />
	<!-- TODO fix this later -->
	<suppress checks="MagicNumber" files=".*\.java" />
	<suppress checks="WhitespaceAround" files=".*\.java" />
	<suppress checks="JavadocMethod" files=".*\.java" />
	<suppress checks="HiddenField" files=".*\.java" />
	<suppress checks="RegexpSingleline" files=".*\.java" />
</suppressions>
